<?xml version="1.0" encoding="UTF-8"?>
<beans	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:mvc="http://www.springframework.org/schema/mvc"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans-4.1.xsd
	http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.1.xsd 
	http://www.springframework.org/schema/mvc http://www.springframework.org/schema/mvc/spring-mvc-4.1.xsd" >

	<!-- 各种自定义View 都需要绑定自己的Viewer,而且Viewer必须继承org.springframework.web.servlet.view 中的一种 -->
	<bean id="rssview" class="com.ricardo.springmvc.lesson6.utils.UserRSSView"></bean>
	<bean name="pdfview" class="com.ricardo.springmvc.lesson8.utils.UserPDFView"></bean>
	<bean name="xlsview" class="com.ricardo.springmvc.lesson8.utils.UserExcelView"></bean>
	

	
	
	
	</beans>